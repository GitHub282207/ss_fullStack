@import "fn"

html 
//  100% 关闭字体大小自动调整功能.
    -ms-text-size-adjust 100%
    -webkit-text-size-adjust 100%
body /*$表示变量*/
    line-height 1.6
    font-family $weuiFontDefault
*
    margin 0
    padding 0
a img 
    border 0
a 
    text-decoration none
    setTapColor()
input,textarea
    caret-color $weuiColorPrimary